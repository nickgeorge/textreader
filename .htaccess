RewriteEngine on
RewriteRule ^textreader/wordcount$ word_count.cgi [L]
RewriteRule ^textreader(/.*)?$ context.cgi [L]


RewriteCond   %{REQUEST_URI}   !.*soy\.js
RewriteCond   %{REQUEST_URI}   !/client/*
RewriteRule ^(.*\.(js|css))$ /client/$1 [L]

RewriteCond   %{REQUEST_URI}   !/client/soy/gen/*
RewriteRule ^(.*\.soy\.js)$ /client/soy/gen/$1 [L]

